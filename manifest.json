{
  "manifest_version": 3,
  "name": "Extrator de Variáveis Azure DevOps - Library",
  "version": "1.0",
  "description": "Extrai variáveis de uma tabela do Azure DevOps e copia como JSON.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extrair Variáveis"
  },
  "host_permissions": [
    "https://dev.azure.com/*",
    "https://*.visualstudio.com/*",
    "https://portal.azure.com/*"
  ]
}
